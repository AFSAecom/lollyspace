{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview --port 4173 --strictPort",
    "ci:install": "npm ci --no-audit --no-fund --ignore-scripts",
    "ci:verify-clean": "git update-index -q --refresh && test -z \"$(git status --porcelain)\" || (git status --porcelain; exit 1)",
    "postinstall": "if [ -z \"$CI\" ] && [ -z \"$READONLY_SETUP\" ]; then playwright install --with-deps || true; fi",
    "prepare": "if [ -z \"$CI\" ] && [ -z \"$READONLY_SETUP\" ]; then husky install || true; fi",
    "fix": "eslint . --ext .ts,.tsx --fix && prettier -w .",
    "e2e": "npx playwright test",
    "e2e:ci": "npx playwright test --reporter=line"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "zustand": "^4.4.0",
    "recharts": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.1.0",
    "vite": "^5.0.0"
  },
  "types": "./src"
}
